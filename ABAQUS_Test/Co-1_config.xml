<?xml version="1.0" encoding="utf-8"?><?xml-stylesheet type='text/xsl' href='cse_config_transform.xsl'?><CoupledMultiphysicsSimulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" noNamespaceSchemaLocation="http://www.simulia.com SMACseConfig.xsd"> <header>    <SchemaVersion>1.0</SchemaVersion>  </header>  <components>    <component name="Abaqus/Standard">      <bottomUpImplementation>        <codeName>AbaqusStandard</codeName>      </bottomUpImplementation>    </component>    <component name="Abaqus/Cfd">      <bottomUpImplementation>        <codeName>AbaqusCfd</codeName>      </bottomUpImplementation>    </component>  </components>  <componentInstances>    <componentInstance name="Co-execution-1-Std">      <component>Abaqus/Standard</component>    </componentInstance>    <componentInstance name="Co-execution-1-CFD">      <component>Abaqus/Cfd</component>      <initialConditions>        <sendBeforeReceive>false</sendBeforeReceive>      </initialConditions>    </componentInstance>  </componentInstances>  <connectors>    <connector name="STD_to_CFD_INPUT">      <componentInstance>Co-execution-1-CFD</componentInstance>      <variables>        <input>          <variable>displacement</variable>          <variable>velocity</variable>        </input>      </variables>    </connector>    <connector name="STD_to_CFD_OUTPUT">      <componentInstance>Co-execution-1-Std</componentInstance>      <variables>        <output>          <variable>displacement</variable>          <variable>velocity</variable>        </output>      </variables>    </connector>    <connector name="CFD_to_STD_INPUT">      <componentInstance>Co-execution-1-Std</componentInstance>      <variables>        <input>          <variable>force</variable>          <variable>mass</variable>        </input>      </variables>    </connector>    <connector name="CFD_to_STD_OUTPUT">      <componentInstance>Co-execution-1-CFD</componentInstance>      <variables>        <output>          <variable>traction_vector</variable>          <variable>mass</variable>        </output>      </variables>    </connector>  </connectors>  <connectionSets>    <connectionSet name="STD_to_CFD" type="FIELD">      <connection>        <connector>STD_to_CFD_INPUT</connector>        <connector>STD_to_CFD_OUTPUT</connector>      </connection>    </connectionSet>    <connectionSet name="CFD_to_STD" type="FIELD">      <connection>        <connector>CFD_to_STD_INPUT</connector>        <connector>CFD_to_STD_OUTPUT</connector>      </connection>    </connectionSet>  </connectionSets>  <execution>    <compositeActors>      <compositeActor name="twoCodeContinuousTime">        <actors>          <atomicActor>Co-execution-1-Std</atomicActor>          <atomicActor>Co-execution-1-CFD</atomicActor>        </actors>        <modelOfComputation>          <continuousTime>            <algorithm>GAUSS-SEIDEL</algorithm>            <negotiationMethod>MIN</negotiationMethod>          </continuousTime>        </modelOfComputation>      </compositeActor>    </compositeActors>    <connectionGroups>      <connectionCategory name="InitialConditions">        <connectionSet>STD_to_CFD</connectionSet>        <connectionSet>CFD_to_STD</connectionSet>      </connectionCategory>      <connectionCategory name="CouplingStep">        <connectionSet>STD_to_CFD</connectionSet>        <connectionSet>CFD_to_STD</connectionSet>      </connectionCategory>    </connectionGroups>    <scenario>      <duration>0.2</duration>    </scenario>  </execution></CoupledMultiphysicsSimulation>
